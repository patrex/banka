{
  "name": "banka",
  "version": "1.0.0",
  "description": "banka is a light weight banking service that powers common banking needs",
  "main": "index.js",
  "dependencies": {
    "@babel/runtime": "^7.4.3",
    "bcrypt": "^3.0.5",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "nodemailer": "^6.1.0",
    "pg": "^7.10.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.2",
    "codeclimate-test-reporter": "^0.5.1",
    "coveralls": "^3.0.3",
    "cross-env": "^5.2.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^14.0.0"
  },
  "scripts": {
    "test": "npm run build && npm run migrations && cross-env NODE_ENV=test nyc --reporter=text mocha ./build/test/test.js --exit",
    "build": "babel ./server -d ./build",
    "start": "npm run build && node ./build/server",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint server/**/*.js --ignore-pattern test/",
    "migrations": "cross-env NODE_ENV=test node ./build/models/migrations"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/patrex/banka.git"
  },
  "keywords": [
    "mobile",
    "banking",
    "'mobile",
    "banking'",
    "'internet",
    "banking'"
  ],
  "author": "Patsoks Patsokari",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/patrex/banka/issues"
  },
  "homepage": "https://github.com/patrex/banka#readme"
}
